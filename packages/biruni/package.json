{
	"$schema": "https://json.schemastore.org/package",
	"name": "biruni",
	"type": "module",
	"version": "0.0.3",
	"description": "(Al-Biruni) Unified All-in-One Storage Utilities",
	"keywords": [
		"wonize",
		"biruni",
		"al-biruni"
	],
	"author": "mikoloism <mikoloism@gmail.com>",
	"license": "GPL-3.0-only",
	"homepage": "https://wonize.github.io/biruni/",
	"bugs": "https://github.com/wonize/biruni/issues",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/wonize/biruni.git",
		"directory": "packages/biruni"
	},
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": false,
	"exports": {
		".": {
			"require": "./dist/cjs/mod.js",
			"import": "./dist/esm/mod.js",
			"types": "./dist/types/mod.d.ts"
		},
		"./mod": {
			"require": "./dist/cjs/mod.js",
			"import": "./dist/esm/mod.js",
			"types": "./dist/types/mod.d.ts"
		},
		"./built-in": {
			"require": "./dist/cjs/built-in.js",
			"import": "./dist/esm/built-in.js",
			"types": "./dist/types/built-in.d.ts"
		},
		"./*": {
			"require": "./dist/cjs/*.js",
			"import": "./dist/esm/*.js",
			"types": "./dist/types/*.d.ts"
		}
	},
	"main": "dist/cjs/mod.js",
	"module": "dist/esm/mod.js",
	"browser": "dist/umd/mod.global.js",
	"types": "dist/types/mod.d.ts",
	"files": [
		"dist",
		"LICENSE",
		"README.md",
		"package.json"
	],
	"packageManager": "pnpm@8.9.0",
	"engines": {
		"node": ">=18"
	},
	"scripts": {},
	"dependencies": {
		"@biruni/core": "workspace:*",
		"@biruni/factory": "workspace:*",
		"@biruni/built-in": "workspace:*"
	},
	"devDependencies": {},
	"peerDependencies": {},
	"peerDependenciesMeta": {}
}
