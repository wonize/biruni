{
	"name": "biruni",
	"type": "module",
	"version": "0.0.2",
	"description": "(Al-Biruni) unified store/storage management tool",
	"author": "mikoloism <mikoloism@gmail.com>",
	"license": "GPL-3.0-only",
	"homepage": "https://github.com/wonize/biruni/tree/main/packages/biruni#readme",
	"repository": {
