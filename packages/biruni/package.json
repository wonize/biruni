{
	"name": "biruni",
	"type": "module",
	"version": "0.0.3",
	"description": "(Al-Biruni) unified store/storage management tool",
	"keywords": [
		"wonize",
		"biruni",
		"al-biruni",
		"localstorage",
		"persist"
	],
	"exports": {
		".": {
			"import": {
				"types": "./dist/mod.d.ts",
				"default": "./dist/mod.mjs"
			},
			"require": {
				"types": "./dist/mod.d.cts",
				"default": "./dist/mod.cjs"
			},
			"default": "./dist/mod.mjs",
			"types": "./dist/mod.d.ts"
		},
		"./built-in": {
			"import": {
				"types": "./dist/built-in.d.ts",
				"default": "./dist/built-in.mjs"
			},
			"require": {
				"types": "./dist/built-in.d.cts",
				"default": "./dist/built-in.cjs"
			},
			"default": "./dist/built-in.mjs",
			"types": "./dist/built-in.d.ts"
		}
	},
	"main": "dist/mod.mjs",
	"module": "dist/mod.mjs",
	"types": "dist/mod.d.ts",
	"scripts": {},
	"dependencies": {
		"@biruni/core": "workspace:*",
		"@biruni/factory": "workspace:*",
		"@biruni/built-in": "workspace:*"
	}
}
