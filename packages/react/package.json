	"main": "dist/mod.mjs",
	"module": "dist/mod.mjs",
	"types": "dist/mod.d.ts",
	"scripts": {},
	"devDependencies": {
		"@types/react": "18.2.79",
		"react": "^18.2.0",
		"@biruni/core": "workspace:*"
	},
	"peerDependencies": {
		"react": ">=18"
	},
	"peerDependenciesMeta": {
		"react": {
			"optional": false
		},
		"@biruni/core": {
			"optional": true
		}
	}
