{
	"$schema": "https://json.schemastore.org/package",
	"name": "@biruni/core",
	"type": "module",
	"version": "0.0.5",
	"description": "(Al-Biruni) Core Utilities",
	"keywords": [
		"wonize",
		"biruni",
		"al-biruni"
	],
	"author": "mikoloism <mikoloism@gmail.com>",
	"license": "GPL-3.0-only",
	"homepage": "https://wonize.github.io/biruni/",
	"bugs": "https://github.com/wonize/biruni/issues",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/wonize/biruni.git",
		"directory": "packages/core"
	},
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": false,
	"exports": {
		".": {
			"require": "./dist/cjs/mod.js",
			"import": "./dist/esm/mod.js",
			"types": "./dist/types/mod.d.ts"
		},
		"./mod": {
			"require": "./dist/cjs/mod.js",
			"import": "./dist/esm/mod.js",
			"types": "./dist/types/mod.d.ts"
		},
		"./store": {
			"require": "./dist/cjs/store.js",
			"import": "./dist/esm/store.js",
			"types": "./dist/types/store.d.ts"
		},
		"./set": {
			"require": "./dist/cjs/set/mod.js",
			"import": "./dist/esm/set/mod.js",
			"types": "./dist/types/set/mod.d.ts"
		},
		"./get": {
			"require": "./dist/cjs/get/mod.js",
			"import": "./dist/esm/get/mod.js",
			"types": "./dist/types/get/mod.d.ts"
		},
		"./listener": {
			"require": "./dist/cjs/listener/mod.js",
			"import": "./dist/esm/listener/mod.js",
			"types": "./dist/types/listener/mod.d.ts"
		},
		"./helpers": {
			"require": "./dist/cjs/helpers/mod.js",
			"import": "./dist/esm/helpers/mod.js",
			"types": "./dist/types/helpers/mod.d.ts"
		},
		"./plugin": {
			"require": "./dist/cjs/plugin/mod.js",
			"import": "./dist/esm/plugin/mod.js",
			"types": "./dist/types/plugin/mod.d.ts"
		},
		"./*": {
			"require": "./dist/cjs/*.js",
			"import": "./dist/esm/*.js",
			"types": "./dist/types/*.d.ts"
		}
	},
	"main": "dist/cjs/mod.js",
	"module": "dist/esm/mod.js",
	"browser": "dist/umd/mod.global.js",
	"types": "dist/types/mod.d.ts",
	"files": [
		"dist",
		"LICENSE",
		"README.md",
		"package.json"
	],
	"packageManager": "pnpm@9.2.0",
	"engines": {
		"node": ">=18"
	},
	"scripts": {},
	"dependencies": {
		"dot-prop": "8.0.2"
	},
	"peerDependenciesMeta": {}
}
