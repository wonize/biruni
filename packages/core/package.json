{
	"$schema": "https://json.schemastore.org/package",
	"name": "@biruni/core",
	"type": "module",
	"version": "0.0.5",
	"description": "(Al-Biruni) Core Utilities",
	"keywords": [
		"wonize",
		"biruni",
		"al-biruni"
	],
	"author": "mikoloism <mikoloism@gmail.com>",
	"license": "GPL-3.0-only",
	"homepage": "https://wonize.github.io/biruni/",
	"bugs": "https://github.com/wonize/biruni/issues",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/wonize/biruni.git",
		"directory": "packages/core"
	},
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": false,
	"exports": {
		".": {
			"require": "./dist/cjs/mod.js",
			"import": "./dist/esm/mod.js",
			"types": "./dist/types/mod.d.ts"
		},
		"./mod": {
			"require": "./dist/cjs/mod.js",
			"import": "./dist/esm/mod.js",
			"types": "./dist/types/mod.d.ts"
		},
		"./store": {
			"require": "./dist/cjs/store.js",
			"import": "./dist/esm/store.js",
			"types": "./dist/types/store.d.ts"
		},
		"./set": {
			"require": "./dist/cjs/set.js",
			"import": "./dist/esm/set.js",
			"types": "./dist/types/set.d.ts"
		},
		"./get": {
			"require": "./dist/cjs/get.js",
			"import": "./dist/esm/get.js",
			"types": "./dist/types/get.d.ts"
		},
		"./listener": {
			"require": "./dist/cjs/listener.js",
			"import": "./dist/esm/listener.js",
			"types": "./dist/types/listener.d.ts"
		},
		"./helpers": {
			"require": "./dist/cjs/helpers/mod.js",
			"import": "./dist/esm/helpers/mod.js",
			"types": "./dist/types/helpers/mod.d.ts"
		},
		"./plugin": {
			"require": "./dist/cjs/plugin/mod.js",
			"import": "./dist/esm/plugin/mod.js",
			"types": "./dist/types/plugin/mod.d.ts"
		},
		"./parser": {
			"require": "./dist/cjs/parser/mod.js",
			"import": "./dist/esm/parser/mod.js",
			"types": "./dist/types/parser/mod.d.ts"
		},
		"./persister": {
			"require": "./dist/cjs/persister/mod.js",
			"import": "./dist/esm/persister/mod.js",
			"types": "./dist/types/persister/mod.d.ts"
		},
		"./synchronizer": {
			"require": "./dist/cjs/synchronizer/mod.js",
			"import": "./dist/esm/synchronizer/mod.js",
			"types": "./dist/types/synchronizer/mod.d.ts"
		},
		"./validator": {
			"require": "./dist/cjs/validator/mod.js",
			"import": "./dist/esm/validator/mod.js",
			"types": "./dist/types/validator/mod.d.ts"
		},
		"./context": {
			"require": "./dist/cjs/context.js",
			"import": "./dist/esm/context.js",
			"types": "./dist/types/context.d.ts"
		},
		"./*": {
			"require": "./dist/cjs/*.js",
			"import": "./dist/esm/*.js",
			"types": "./dist/types/*.d.ts"
		}
	},
	"main": "dist/cjs/mod.js",
	"module": "dist/esm/mod.js",
	"browser": "dist/umd/mod.global.js",
	"types": "dist/types/mod.d.ts",
	"files": [
		"dist",
		"LICENSE",
		"README.md",
		"package.json"
	],
	"packageManager": "pnpm@8.9.0",
	"engines": {
		"node": ">=18"
	},
	"scripts": {},
	"dependencies": {},
	"devDependencies": {},
	"peerDependencies": {},
	"peerDependenciesMeta": {}
}